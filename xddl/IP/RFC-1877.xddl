  <fragment name="Primary DNS Server Address" id="PrimaryDNS_ServerAddress">
    <field name="PrimaryDNS_ServerAddress" length="4" type="IP.xddl#ip_address"/>
  </fragment>

  <fragment name="Primary NBNS Server Address" id="PrimaryNBNS_ServerAddress">
    <field name="PrimaryNBNS_ServerAddress" length="4" type="IP.xddl#ip_address"/>
  </fragment>

  <fragment name="Secondary DNS Server Address" id="SecondaryDNS_ServerAddress">
    <field name="SecondaryDNS_ServerAddress" length="4" type="IP.xddl#ip_address"/>
  </fragment>

  <fragment name="Secondary NBNS Server Address" id="SecondaryNBNS_ServerAddress">
    <field name="SecondaryNBNS_ServerAddress" length="4" type="IP.xddl#ip_address"/>
  </fragment>

</xddl>
